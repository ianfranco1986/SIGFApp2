	<queryString>
		<![CDATA[SELECT 
    b.bus_numero AS numeroPaga,
    b.bus_patente AS patentePaga,
    c.bus_numero AS numeroRecibe,
    c.bus_patente AS patenteRecibe,
   minutos_pagados_bus($P{desde}, $P{hasta}, c.bus_id) AS monto, 
minutos_cobrados_bus($P{desde}, $P{hasta}, c.bus_id) AS cobrado, 

(minutos_cobrados_bus($P{desde}, $P{hasta}, c.bus_id)-minutos_pagados_bus($P{desde}, $P{hasta}, c.bus_id) ) AS saldo
FROM
	recaudacion_minuto rm
		LEFT JOIN 
    recaudacion rr ON rm.recaudacion_minuto_id_recaudacion = rr.recaudacion_id 
		LEFT JOIN 
    registro_minuto r ON rm.recaudacion_minuto_id_registro_minuto = r.registro_minuto_id
        LEFT JOIN
    bus b ON r.registro_minuto_desde_id_bus = b.bus_id
        LEFT JOIN
    bus c ON r.registro_minuto_hasta_id_bus = c.bus_id
WHERE
    rr.recaudacion_fecha BETWEEN  $P{desde} AND $P{hasta}  AND  c.bus_id_empresa =   $P{codigo}  GROUP BY c.bus_numero ORDER BY c.bus_numero ASC]]>
	</queryString>
