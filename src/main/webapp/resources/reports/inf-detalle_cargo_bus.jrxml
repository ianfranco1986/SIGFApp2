	<queryString>
		<![CDATA[SELECT 
    bus.bus_numero,
    bus.bus_patente,
    tipo_cargo.tipo_cargo_nombre, 
    cargo_bus.cargo_bus_descripcion,
    cargo_bus.cargo_bus_monto_fijo,
    cargo_bus.cargo_bus_cuota_actual,
    cargo_bus.cargo_bus_total_cuotas,
    cargo_bus.cargo_bus_fecha_inicio,
    cargo_bus.cargo_bus_fecha_termino,
    empresa.empresa_nombre
FROM
   cargo_bus
        LEFT JOIN
    bus ON cargo_bus.cargo_bus_id_bus = bus.bus_id
        LEFT JOIN
    empresa ON bus.bus_id_empresa = empresa.empresa_id
        LEFT JOIN
    tipo_cargo ON cargo_bus.cargo_bus_id_bus = tipo_cargo.tipo_cargo_id
WHERE
  cargo_bus.cargo_bus_fecha_inicio BETWEEN $P{desde} AND $P{hasta}
        AND cargo_bus.cargo_bus_id_tipo = $P{tipoCargo}
ORDER BY bus.bus_numero]]>
	</queryString>
